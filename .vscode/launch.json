  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/example",
      "args": [
        "--device-id",
        "chrome",
        "--web-browser-flag",
        "--disable-web-security",
        "--web-hostname",
        "0.0.0.0",
        "--web-port",
        "8080",
        "-v"
      ],
    },
  ]
}